package app;

import java.io.File;
import java.util.*;


public class Archivio {
	private static File file = new File("file.txt");
	private Map<Long,Pubblicazione> archivio;
	
	public Archivio() {
		this.archivio = new HashMap<Long, Pubblicazione>();
	}
	
	public Map<Long, Pubblicazione> getArchivio() {
		return archivio;
	}
	
	public nuovaPubblicazione(Pubblicazione o) {
		Pubblicazione pubblicazioneInserita = archivio.put(null, o)
	}
}
